{
  "name": "Dictionary",
  "description": "Dictionary (chrome extension) is an simple dictionary.",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {"128":"./img/icon.png"},

  "browser_action":{
    "default_icon": "./img/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "./img/background.jpg"
  ],
  "background":{"scripts": ["background.js"]},
  "permissions":[
    "tabs",
		"activeTab",
    "http://glosbe.com/*",
    "https://glosbe.com/*",
    "http://google.com/*",
    "https://google.com/*"
	]
}
