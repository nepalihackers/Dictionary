{
  "name": "Dictionary",
  "description": "Dictionary chrome extenction to easily search for words and their meaning",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {"128":"./img/icon.png"},

  "browser_action":{
    "default_icon": "./img/icon.png",
    "default_popup": "popup.html"
  },

  "permissions":[
    "tabs",
		"activeTab",
    "http://glosbe.com/*",
    "https://glosbe.com/*",
    "http://google.com/*",
    "https://google.com/*"
	]
}
